programa 
{
	
	funcao inicio()
	{
		real n1, n2, n3, media
		escreva("Digite os três números: ")
		leia(n1,n2,n3)
		media=(n1+n2+n3)/3
		escreva("A média dos números digitados é ",media)
	}
}
